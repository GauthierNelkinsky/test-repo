#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/nuxt@3.11.2_@opentelemetry+api@1.8.0_@parcel+watcher@2.4.1_@types+node@20.12.12_@unocss+reset_fzxfrmak4qhhuhcasdva7axugu/node_modules/nuxt/bin/node_modules:/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/nuxt@3.11.2_@opentelemetry+api@1.8.0_@parcel+watcher@2.4.1_@types+node@20.12.12_@unocss+reset_fzxfrmak4qhhuhcasdva7axugu/node_modules/nuxt/node_modules:/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/nuxt@3.11.2_@opentelemetry+api@1.8.0_@parcel+watcher@2.4.1_@types+node@20.12.12_@unocss+reset_fzxfrmak4qhhuhcasdva7axugu/node_modules:/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/nuxt@3.11.2_@opentelemetry+api@1.8.0_@parcel+watcher@2.4.1_@types+node@20.12.12_@unocss+reset_fzxfrmak4qhhuhcasdva7axugu/node_modules/nuxt/bin/node_modules:/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/nuxt@3.11.2_@opentelemetry+api@1.8.0_@parcel+watcher@2.4.1_@types+node@20.12.12_@unocss+reset_fzxfrmak4qhhuhcasdva7axugu/node_modules/nuxt/node_modules:/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/nuxt@3.11.2_@opentelemetry+api@1.8.0_@parcel+watcher@2.4.1_@types+node@20.12.12_@unocss+reset_fzxfrmak4qhhuhcasdva7axugu/node_modules:/home/runner/work/dm-frontend-pwa/dm-frontend-pwa/src/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
